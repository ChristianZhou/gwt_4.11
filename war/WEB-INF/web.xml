<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">



    <servlet>
        <servlet-name>customerServiceRemote</servlet-name>
        <servlet-class>com.mySampleApplication.server.services.CustomerServiceRemoteImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>customerServiceRemote</servlet-name>
        <url-pattern>/MySampleApplication/customerServiceRemote</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>customerTypeService</servlet-name>
        <servlet-class>com.mySampleApplication.server.services.CustomerTypeServiceImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>customerTypeService</servlet-name>
        <url-pattern>/MySampleApplication/customerTypeService</url-pattern>
    </servlet-mapping>


</web-app>